<%= form_with(model: list, local: true) do |form| %>

  <div class="form-group">
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>

  <div class="form-group">
    <%= form.hidden_field :user_id, value: current_user.id %>
  </div>

  <div class="actions">
    <%= form.submit class: "btn btn-primary" %>
  </div>
<% end %>
